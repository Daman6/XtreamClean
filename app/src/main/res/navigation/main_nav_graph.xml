<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.xtreamclean.ui.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_customBottomSheetDialogFragment"
            app:destination="@id/customBottomSheetDialogFragment" />
        <action
            android:id="@+id/action_homeFragment_to_offerFragment"
            app:destination="@id/offerFragment" />
        <action
            android:id="@+id/action_homeFragment_to_myHistoryFragment"
            app:destination="@id/myHistoryFragment" />
        <action
            android:id="@+id/action_homeFragment_to_myHistoryFragment2"
            app:destination="@id/myHistoryFragment" />
        <action
            android:id="@+id/action_homeFragment_to_taskDetailFragment"
            app:destination="@id/taskDetailFragment" />
        <action
            android:id="@+id/action_homeFragment_to_headerBottomFragment"
            app:destination="@id/headerBottomFragment" />
    </fragment>
    <dialog
        android:id="@+id/customBottomSheetDialogFragment"
        android:name="com.example.xtreamclean.bottomsheet.CustomBottomSheetDialogFragment"
        android:label="CustomBottomSheetDialogFragment"
        tools:layout="@layout/sortby_bottomsheet_layout"/>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.xtreamclean.ui.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_loginFragment_to_forgetFragment"
            app:destination="@id/forgetFragment" />
        <action
            android:id="@+id/action_loginFragment_to_registrationFragment"
            app:destination="@id/registrationFragment" />
    </fragment>
    <fragment
        android:id="@+id/forgetFragment"
        android:name="com.example.xtreamclean.ui.ForgetFragment"
        android:label="fragment_forget"
        tools:layout="@layout/fragment_forget" />
    <fragment
        android:id="@+id/offerFragment"
        android:name="com.example.xtreamclean.ui.OfferFragment"
        android:label="fragment_offer"
        tools:layout="@layout/fragment_offer" />
    <fragment
        android:id="@+id/myHistoryFragment"
        android:name="com.example.xtreamclean.ui.MyHistoryFragment"
        android:label="MyHistoryFragment"
        tools:layout="@layout/fragment_my_history"/>
    <fragment
        android:id="@+id/taskDetailFragment"
        android:name="com.example.xtreamclean.ui.TaskDetailFragment"
        android:label="fragment_task_detail"
        tools:layout="@layout/fragment_task_detail" >
        <action
            android:id="@+id/action_taskDetailFragment_to_costFragment"
            app:destination="@id/costFragment" />
        <action
            android:id="@+id/action_taskDetailFragment_to_reservationNotesFragment"
            app:destination="@id/reservationNotesFragment" />
        <action
            android:id="@+id/action_taskDetailFragment_to_summaryFragment"
            app:destination="@id/summaryFragment" />
        <action
            android:id="@+id/action_taskDetailFragment_to_issuesFragment"
            app:destination="@id/issuesFragment" />
        <action
            android:id="@+id/action_taskDetailFragment_to_detailFragment"
            app:destination="@id/detailFragment" />
        <action
            android:id="@+id/action_taskDetailFragment_to_productDetailFragment"
            app:destination="@id/productDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/costFragment"
        android:name="com.example.xtreamclean.ui.CostFragment"
        android:label="fragment_cost"
        tools:layout="@layout/fragment_cost">
        <action
            android:id="@+id/action_costFragment_to_costFilterFragment"
            app:destination="@id/costFilterFragment" />
    </fragment>
    <fragment
        android:id="@+id/headerBottomFragment"
        android:name="com.example.xtreamclean.ui.HeaderBottomFragment"
        android:label="fragment_header_bottom"
        tools:layout="@layout/fragment_header_bottom" >
        <action
            android:id="@+id/action_headerBottomFragment_to_labsFragment"
            app:destination="@id/labsFragment" />
        <action
            android:id="@+id/action_headerBottomFragment_to_notificationFragment"
            app:destination="@id/notificationFragment" />
        <action
            android:id="@+id/action_headerBottomFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_headerBottomFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
    </fragment>
    <fragment
        android:id="@+id/issuesFragment"
        android:name="com.example.xtreamclean.ui.IssuesFragment"
        android:label="IssuesFragment"
        tools:layout="@layout/fragment_issues"/>
    <fragment
        android:id="@+id/summaryFragment"
        android:name="com.example.xtreamclean.ui.SummaryFragment"
        android:label="fragment_summary"
        tools:layout="@layout/fragment_summary" />
    <fragment
        android:id="@+id/reservationNotesFragment"
        android:name="com.example.xtreamclean.ui.ReservationNotesFragment"
        android:label="fragment_reservation_notes"
        tools:layout="@layout/fragment_reservation_notes" />
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.example.xtreamclean.ui.DetailFragment"
        android:label="fragment_detail"
        tools:layout="@layout/fragment_detail" />
    <fragment
        android:id="@+id/labsFragment"
        android:name="com.example.xtreamclean.ui.LabsFragment"
        android:label="fragment_labs"
        tools:layout="@layout/fragment_labs" />
    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.example.xtreamclean.ui.NotificationFragment"
        android:label="NotificationFragment"
        tools:layout="@layout/fragment_notification"
        />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.xtreamclean.ui.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/productDetailFragment"
        android:name="com.example.xtreamclean.ui.ProductDetailFragment"
        android:label="ProductDetailFragment"
        tools:layout="@layout/fragment_product_detail">
        <action
            android:id="@+id/action_productDetailFragment_to_elementDetailFragment"
            app:destination="@id/elementDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/elementDetailFragment"
        android:name="com.example.xtreamclean.ui.ElementDetailFragment"
        android:label="ElementDetailFragment"
        tools:layout="@layout/fragment_element_detail"/>
    <fragment
        android:id="@+id/costFilterFragment"
        android:name="com.example.xtreamclean.ui.CostFilterFragment"
        android:label="CostFilterFragment"
        tools:layout="@layout/fragment_cost_filter"/>
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.example.xtreamclean.ui.ChatFragment"
        android:label="ChatFragment"
        tools:layout="@layout/fragment_chat"/>
    <fragment
        android:id="@+id/registrationFragment"
        android:name="com.example.xtreamclean.ui.RegistrationFragment"
        android:label="RegistrationFragment"
        tools:layout="@layout/fragment_registration">
        <action
            android:id="@+id/action_registrationFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_registrationFragment_to_otpFragment"
            app:destination="@id/otpFragment" />
    </fragment>
    <fragment
        android:id="@+id/otpFragment"
        android:name="com.example.xtreamclean.ui.OtpFragment"
        android:label="OtpFragment"
        tools:layout="@layout/fragment_otp">
        <action
            android:id="@+id/action_otpFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>